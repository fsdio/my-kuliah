<script>
    const gotoPage = (link) => window.location.href = link
    const jump = (h) => {
        let top = document.getElementById(h).offsetTop
        window.scrollTo({left: 0, top: top - 100, behavior: 'smooth'})
    }

    $(document).ready(() => {
        $('#article .content a').attr('target', '_blank');
        // sources
        if ($('#article .dxsources-item a').length > 0) {
            $('#article .content .dxsources').before('<h3 style="margin-bottom:-16px">Sumber</h3>')
        }
        // table of content
        if ($('#article .content h2').length < 1) {
            $('#article .toc').hide();
        }
        $('#article .content h2').each(function(index, item) {
            const text = $(item).text();
            const link = text.replace(/\s+/g, '-').replace(/\?/g, '').toLowerCase();
            
            $(item).attr('id', link)
            const el = `
            <li>
                <a href="#${link}">
                    ${$(item).text()}
                </a>
            </li>
            `
            $('#article .toc ol').append(el)
        })
        $('#article .dxsources-item a').attr('target', '_blank');
        $('#article .frame').each(function(index, el) {
            $(el).Lazy();
        })
        $('#article img').each(function(index, el) {
            $(el).Lazy();
        })
        $('#article iframe').each(function(index, el) {
            $(el).Lazy();
        })
        // reverse category
        let breadcrumbs = []
        $('#article .breadcrumb a').each(function(index, el) {
            breadcrumbs.push($(el)[0].outerHTML)
        })
        // breadcrumbs = breadcrumbs.reverse()
        $('#article .breadcrumb').html(breadcrumbs.join(' <i class="fas fa-chevron-right" style="color: #c6c6c6"></i> '))
    })
</script>
